{
  "name": "n8n-nodes-postnl",
  "version": "0.1.0",
  "description": "n8n node to connect with PostNL API",
  "keywords": [
    "n8n-community-node-package"
  ],
  "license": "MIT",
  "homepage": "",
  "author": {
    "name": "lennartki",
    "email": ""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lennartki/n8n-nodes-postnl.git"
  },
  "engines": {
    "node": ">=18.10",
    "pnpm": ">=9.1"
  },
  "packageManager": "pnpm@9.1.4",
  "main": "index.js",
  "scripts": {
    "build": "gulp build:icons && tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/PostNLApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/PostNL/PostNL.node.js"
    ]
  },
  "devDependencies": {
    "@eslint/css": "^0.9.0",
    "@eslint/js": "^9.29.0",
    "@eslint/json": "^0.12.0",
    "@eslint/markdown": "^6.5.0",
    "@types/jest": "^29.5.12",
    "@typescript-eslint/parser": "^7.15.0",
    "eslint": "^8.57.1",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "globals": "^16.2.0",
    "gulp": "^4.0.2",
    "jest": "^29.7.0",
    "n8n-workflow": "*",
    "prettier": "^3.3.2",
    "ts-jest": "^29.2.5",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.34.1"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "dependencies": {
    "@devlikeapro/n8n-openapi-node": "^0.1.0",
    "gulp-cli": "^3.1.0",
    "n8n-workflow": "*",
    "tsc": "^2.0.4"
  }
}
